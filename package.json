{
  "name": "transcribe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cdk": "cd infrastructure && cdk",
    "cdk:dev": "npm run cdk -- deploy --context env=dev",
    "cdk:staging": "npm run cdk -- deploy --context env=staging",
    "cdk:prod": "npm run cdk -- deploy --context env=prod",
    "cdk:diff": "npm run cdk -- diff",
    "cdk:synth": "npm run cdk -- synth",
    "cdk:bootstrap": "npm run cdk -- bootstrap",
    "cdk:destroy": "npm run cdk -- destroy",
    "cdk:destroy:dev": "npm run cdk -- destroy --context env=dev",
    "cdk:destroy:staging": "npm run cdk -- destroy --context env=staging",
    "cdk:destroy:prod": "npm run cdk -- destroy --context env=prod"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.816.0",
    "@aws-sdk/lib-dynamodb": "^3.816.0",
    "@clerk/nextjs": "^6.9.2",
    "next": "15.0.3",
    "openai": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "aws-cdk": "^2.1016.1",
    "aws-cdk-lib": "^2.197.0",
    "constructs": "^10.4.2",
    "daisyui": "^5.0.35",
    "esbuild": "^0.25.4",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^4.1.7",
    "typescript": "^5"
  }
}
